        workspace =pwd()
    }
    stage('Static Code Analysis')
    {
        echo "Static Code Analysis"
    }
  stage('Build')
  {
       echo "Build the code"
  }
  
    stage('Unit testing')
    {
        echo "Unit testing"
    }
    stage('Delivery')
    {
        echo "Deliver the code"
    }
